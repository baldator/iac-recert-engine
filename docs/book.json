{
  "title": "IaC Recertification Engine Documentation",
  "description": "Comprehensive documentation for the IaC Recertification Engine (ICE)",
  "author": "IaC Recertification Engine Team",
  "language": "en",
  "gitbook": ">=3.0.0",
  "plugins": [
    "anchors",
    "copy-code-button",
    "edit-link",
    "github",
    "github-buttons",
    "prism",
    "-highlight",
    "search",
    "sharing",
    "splitter",
    "toggle-chapters"
  ],
  "pluginsConfig": {
    "edit-link": {
      "base": "https://github.com/baldator/iac-recert-engine/edit/main/docs",
      "label": "Edit This Page"
    },
    "github": {
      "url": "https://github.com/baldator/iac-recert-engine"
    },
    "github-buttons": {
      "repo": "baldator/iac-recert-engine",
      "types": ["star", "watch", "fork"],
      "size": "small"
    },
    "sharing": {
      "facebook": false,
      "twitter": true,
      "google": false,
      "weibo": false,
      "instapaper": false,
      "vk": false,
      "all": ["twitter"]
    }
  },
  "links": {
    "sidebar": {
      "Home": "https://github.com/baldator/iac-recert-engine",
      "GitHub": "https://github.com/baldator/iac-recert-engine",
      "Issues": "https://github.com/baldator/iac-recert-engine/issues"
    }
  },
  "pdf": {
    "pageNumbers": true,
    "fontSize": 12,
    "paperSize": "a4",
    "margin": {
      "right": 62,
      "left": 62,
      "top": 56,
      "bottom": 56
    }
  }
}
